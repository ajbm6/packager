{
    "name": "cleaniquecoders/packager",
    "description": "Laravel Standalone Package Creator: Use for Your Laravel Package Developement",
    "keywords": ["laravel","package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Console\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/filesystem": "^5.5",
        "illuminate/support": "^5.5",
        "symfony/console": "^3.2|~4.0",
        "symfony/filesystem": "^3.2|~4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^6.5",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "packager"
    ]
}
